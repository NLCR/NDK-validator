									<element name="premis:formatRegistryKey" mandatory="MANDATORY" />
								</element>
							</element>
							<element name="premis:creatingApplication" mandatory="MANDATORY">
								<element name="premis:creatingApplicationName"
									mandatory="MANDATORY" />
								<element name="premis:creatingApplicationVersion"
									mandatory="MANDATORY" />
								<element name="premis:dateCreatedByApplication"
									mandatory="MANDATORY" />
